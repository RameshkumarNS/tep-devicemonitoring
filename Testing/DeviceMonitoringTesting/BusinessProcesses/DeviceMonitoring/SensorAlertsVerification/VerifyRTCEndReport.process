    <pd:transition>
        <pd:from>Assign device info</pd:from>
        <pd:to>Assign-1</pd:to>
        <pd:xpathDescription>Error re WER1</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>(  (abs($deviceInformation/group/dataset/point15 - $deviceInformation/group/Parameters/mean) &gt; abs($deviceInformation/group/Parameters/threeSigma) - $deviceInformation/group/Parameters/mean)
or
(abs($deviceInformation/group/dataset/point15 - $deviceInformation/group/Parameters/mean) &gt; abs($deviceInformation/group/Parameters/neg_threeSigma - $deviceInformation/group/Parameters/mean))
) 
 !=(contains(tib:trim($deviceInformation/group/RTCReported_WERFired), "WER1"))</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Set data points count</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
